{
  "name": "codebase-frontend",
  "version": "0.2.0",
  "description": "Source code of Front End's codebase",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "build": "babel-node tools/build.js",
    "build:development": "npm run build -- --development",
    "build:production": "npm run build -- --production",
    "build:staging": "npm run build -- --staging",
    "dev": "concurrently -k -r \"npm:open:src\" \"npm:lint:watch\"",
    "lint": "esw src --color --ignore-pattern src/assets",
    "lint:watch": "npm run lint -- --watch",
    "open:src": "babel-node tools/srcServer.js",
    "start": "babel-node tools/distServer.js",
    "test": "jest --passWithNoTests",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --watch"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fontsource/ubuntu": "^4.2.1",
    "@hot-loader/react-dom": "^17.0.1",
    "@reduxjs/toolkit": "^1.7.1",
    "axios": "^0.24.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^6.2.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/node": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-do-expressions": "^7.12.13",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.12.13",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.12.16",
    "@babel/plugin-proposal-pipeline-operator": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-react-constant-elements": "^7.12.13",
